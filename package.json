{
    "name": "graphql-workshop",
    "version": "1.0.0",
    "description": "Demo package for the z+ prs graphql workshop",
    "main": "index.js",
    "author": "Roman Decker<Roman.Decker@zooplus.com>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start": "npm run dev",
        "dev": "run-p build:watch watch",
        "build": "babel src/index.js --out-file build/index.js --source-maps",
        "build:watch": "npm run build -- --watch",
        "watch": "nodemon --watch build build/index.js",
        "postinstall": "npm run build"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "express": "^4.16.3",
        "graphql": "^0.13.2",
        "nodemon": "^1.17.5",
        "npm-run-all": "^4.1.3"
    },
    "dependencies": {
        "source-map-support": "^0.5.6"
    }
}
